#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/bin/osascript" ]; then
  "$basedir//usr/bin/osascript"  "$basedir/../browse/script/cmd+shft+f.applescript" "$@"
  ret=$?
else 
  /usr/bin/osascript  "$basedir/../browse/script/cmd+shft+f.applescript" "$@"
  ret=$?
fi
exit $ret
